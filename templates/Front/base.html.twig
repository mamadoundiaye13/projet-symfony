<!doctype html>
<html lang="en">
<head>
    {%- block head -%}
        <meta charset="UTF-8">
        <meta name="viewport"
              content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
        <meta http-equiv="X-UA-Compatible" content="ie=edge">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}{% endblock %}
    {%- endblock -%}
</head>
<body>
        {% block body %}
                {% block header %} <h1 style="background: red">Header</h1>{% endblock %}
                {% block main %} <h1 style="background: red">Body</h1>{% endblock %}
                {% block feature %} <h1 style="background: red">Footer</h1>{% endblock %}
        {% endblock %}

</body>
</html>

